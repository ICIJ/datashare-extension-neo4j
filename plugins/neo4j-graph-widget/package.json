{
  "name": "datashare-plugin-neo4j-graph-widget",
  "description": "A Datashare plugin to add a neo4j graph widget to the project page",
  "version": "1.1.1",
  "main": "dist/index.umd.cjs",
  "style": "dist/style.css",
  "type": "module",
  "dependencies": {
    "bodybuilder": "^2.5.1",
    "bootstrap-vue-next": "0.28",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@icij/eslint-config-icij": "^1.0.2",
    "@iconify-json/ph": "^1.2.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "sass": "^1.77.5",
    "unplugin-icons": "^23.0.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^5.3.1",
    "vite-plugin-externals": "^0.6.2"
  },
  "private": false,
  "datashare": {},
  "scripts": {
    "build": "npx vite build --mode production",
    "build:pack": "echo \"$(npm pack)\"",
    "build:pack:extract": "mkdir -p ./tmp && cat \"$(npm pack)\" | tar zxvf - -C ./tmp",
    "prepublishOnly": "yarn build",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "license": "MIT",
  "files": [
    "dist"
  ]
}
